{"title": {
      "text": "{{ title  }}"
 },
 "legend": {
      "data": [
        {% for i, k in enumerate(legend) %}
          {% if i == len(legend) - 1 %}
            "{{ k }}"
          {% else %}
            "{{ k }}",
          {% end %}
        {% end %}
      ]
 },
 "xAxis": {
      "name": "时间",
      "type": "time"
 },
 "yAxis": {
      "name": "百分比",
      "type": "value"
 },
  "dataZoom": [
        {
            "type": "slider",
            "start": 90,
            "end": 100
        }
    ],
 "series": [
            {% for i, (k, v) in enumerate(series.items()) %}
              {% if i == len(series) -1 %}
                {"name": "{{ k }}",
                 "type": "line",
                 "data": [
                   {% for j, (d, p) in enumerate(v) %}
                      {% if j == len(v) - 1 %}
                        ["{{ d }}", {{ p }}]

                      {% else %}
                        ["{{ d }}", {{ p }}],
                      {% end %}
                   {% end %}
                         ]
                }
              {% else %}
                {"name": "{{ k }}",
                 "type": "line",
                 "data": [
                   {% for j, (d, p) in enumerate(v) %}
                      {% if j == len(v) - 1 %}
                        ["{{ d }}", {{ p }}]

                      {% else %}
                        ["{{ d }}", {{ p }}],
                      {% end %}
                   {% end %}
                         ]
                },
              {% end %}
            {% end %}
           ]
 }